<%- include("header", {title: title}); %>


<h1>Account details</h1>

<p>Here are the details for account <%= account %>.</p>


<table>
    <tr>
        <th>ID</th><th>Name</th><th>Balance</th>
    </tr>
<% for (const row of res) { %>
    <tr>
        <td><%= row.id %></td>
        <td><%= row.name %></td>
        <td class="right"><%= row.balance %></td>
    </tr>
<% }; %>
</table>
<p>
<a href="/bank/balance/">Tillbaka</a>
</p>
<%- include("footer"); %>
